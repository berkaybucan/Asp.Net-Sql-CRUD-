<h1 class="mx-auto">Istihdam Ekleme Paneli</h1>
<form asp-action="Ekle" method="post">

    <div class="form-group mb-3">
        <label class="control-label" for="TCNO">Personel T.C No:</label>
        <input type="text" name="Tcno" class="form-control" id="TCNO" placeholder="###########" minlength="11" maxlength="11" required>
    </div>
    <div class="form-group mb-3">
        <label class="control-label" for="proje">Proje:</label>
        <input type="number" name="ProjeNo" class="form-control" id="proje" placeholder="2" required>

    </div>

    <div class="form-group mb-3">
        <label class="control-label" for="cSaat">Çalışma Saati: </label>
        <input type="number" name="CalismaSaati" class="form-control" id="cSaat" placeholder="8" required>

    </div>
    <button class="btn btn-success" type="submit">Istihdam Ekle</button>

</form>